{
  "name": "nb-config",
  "version": "0.1.0",
  "description": "simple configuration manager",
  "main": "lib/nb-config.js",
  "scripts": {
    "build": "webpack --env dev --mode development --progress && webpack --env build --mode development -p --progress&& npm run test",
    "dev": "webpack --progress --colors --watch --env dev --mode development",
    "test": "NODE_ENV=test mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noizbuster/nb-config.git"
  },
  "keywords": [
    "noizbuster",
    "nb",
    "nodejs",
    "configuration",
    "config"
  ],
  "author": "NoizBuster <noizbuster@noizbuster.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/noizbuster/nb-config/issues"
  },
  "homepage": "https://github.com/noizbuster/nb-config#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "fs-extra": "^5.0.0",
    "mocha": "^4.0.1",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.5"
  }
}
